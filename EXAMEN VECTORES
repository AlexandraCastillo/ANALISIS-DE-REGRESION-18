#EJERCICIO 1
set.seed(10)
M1<-sample(11:30)
dim(M1)<-c(4,5)
rownames(M1)<-c("TOLUCA","PACHUCA","GDL","MTY")
colnames(M1)<-c("ESCUELA1","ESCUELA2","ESCUELA3","ESCUELA4","ESCUELA5")
M1
#ESCUELA 3 DE TOLUCA
M1[1,3]
#ESCUELA 5 DE GUADALAJARA
M1[3,5]
#ESCUELA 1 DE PACHUCA
M1[2,1]


#EJERCICIO 2 
set.seed(10)
V<-sample(0:99,18,replace=F)
V
M<-matrix(V,nrow=6,ncol=3,byrow=FALSE)
M
M[3,3]


#EJERCICIO 3:COMPLETAR EL CÓDIGO
