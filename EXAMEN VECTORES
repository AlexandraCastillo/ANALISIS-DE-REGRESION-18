class(FACULTAD)
class(LICENCIATURA)
class(CALIFICACION)


#EJERCICIO 6
NOMBRE<-c("CARLOS","JUAN","RAQUEL","PATY")
AÑOS<-c(32,35,43,25)
SEG_SOC<-c("IMMS","IMSS","ISSSTE","PRIVADO")
SIT_CONY<-c("CASADO","SOLTERO","SOLTERO","UNIÓN LIBRE")
OCUPACION<-c("CONTADOR","CARPINTERO","PROFESORA","DISEÑADOR")
DATOS<-data.frame(NOMBRE,AÑOS,SEG_SOC,SIT_CONY,OCUPACION) 
DATOS
#OCUPACION DE JUAN Y PATY
OCUPACION[c(2,4)]
#ENCONTRAR SEGURIDAD SOCIAL DE RAQUEL Y CARLOS
SEG_SOC[c(3,1)]
#SITUACION CONYUGAL DE PATY Y JUAN
SIT_CONY[c(4,2)]
#SUMA DE EDADES
sum(AÑOS)
